<idea-plugin>
    <id>Myplugin</id>
    <name>Practice</name>
    <vendor email="" url="">Github</vendor>

    <description><![CDATA[
    Welcome to use, this is my first plugin!
    ]]></description>

    <!-- please see https://plugins.jetbrains.com/docs/intellij/plugin-compatibility.html
         on how to target different products -->
    <depends>com.intellij.modules.platform</depends>
    <depends>com.intellij.modules.lang</depends>

    <extensions defaultExtensionNs="com.intellij">
        <!-- Add your extensions here -->
    </extensions>

    <actions>
        <!-- Add your actions here -->
        <action id="it.unisa.HelloWorldPlugin.PopupDialogAction" class="it.unisa.HelloWorldPlugin.PopupDialogAction"
                text="HelloWorld" description="a simple plugin">
            <add-to-group group-id="ToolsMenu" anchor="first"/>
            <keyboard-shortcut keymap="$default" first-keystroke="T" second-keystroke="O"/>
        </action>
        <action id="PopupAction" class="it.unisa.HelloWorldPlugin.PopupAction" text="PopupAction">
            <add-to-group group-id="EditorPopupMenu" anchor="first"/>
        </action>
        <action id="MyMenu" class="it.unisa.HelloWorldPlugin.MyMenu" text="MyMenu">
            <add-to-group group-id="MainMenu" anchor="last"/>
        </action>
        <action id="JavaToTxtAction" class="FileManipulation.JavaToTxtAction" text="JavaToTxtAction">
            <add-to-group group-id="ToolsMenu" anchor="last"/>
        </action>
        <action id="ListHttpRequestsAction" class="NetworkManager.ListHttpRequestsAction"
                text="ListHttpRequests">
            <add-to-group group-id="ToolsMenu" anchor="first"/>
        </action>
    </actions>
</idea-plugin>